<?xml version="1.0" encoding="utf-8" standalone="no"?>
<vehicle type="spreader" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
  <storeData>
    <name>
      <en>L3030G4 Drybox</en>
      <de>L3030G4 Trockenkasten</de>
    </name>
    <specs>
      <workingWidth>24</workingWidth>
      <combination xmlFilename="4940.xml"/>
    </specs>
    <functions>
      <function>$l10n_function_fertilizer</function>
    </functions>
    <image>store_3030.dds</image>
    <price>48000</price>
    <lifetime>600</lifetime>
    <rotation>0</rotation>
    <brand>JOHNDEERE</brand>
    <category>fertilizerSpreaders</category>
        <vertexBufferMemoryUsage>476416</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>104960</indexBufferMemoryUsage>
        <textureMemoryUsage>1376256</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
  </storeData>

  <base>
    <typeDesc>$l10n_typeDesc_fertilizerSpreader</typeDesc>
    <filename>L3030G4.i3d</filename>
    <size width="7.0" length="8.0"/>
    <speedLimit value="30"/>

    <components>
      <component centerOfMass="0.00 2.76 0.00" solverIterationCount="10" mass="2000"/>
    </components>

    <schemaOverlay attacherJointPosition="0 0" name="IMPLEMENT" />
    <sounds volumeFactor="1.3" />
  </base>

  <attachable>
    <inputAttacherJoints>
      <inputAttacherJoint node="attacher_joint" jointType="implement" upperRotationOffset="0" lowerRotationOffset="0">
        <distanceToGround lower="0" upper="0"/>
      </inputAttacherJoint>
    </inputAttacherJoints>
    <support animationName="moveSupport"/>
  </attachable>

  <powerConsumer ptoRpm="450" neededMaxPtoPower="10"/>

  <lights>
    <sharedLight linkNode="bigTriangle"       filename="$data/shared/assets/reflectors/lizard/bigTriangle.xml" />

    <realLights>
      <high>
        <light node="backLightsHigh1"        lightTypes="0"/>
        <brakeLight node="backLightsHigh1" />
        <turnLightLeft  node="turnLightLeftBack" />
        <turnLightRight node="turnLightRightBack" />
      </high>
    </realLights>

    <defaultLights>
      <defaultLight shaderNode="rearBreakVisRight" lightTypes="0"/>
      <defaultLight shaderNode="rearBreakVisLeft" lightTypes="0"/>
    </defaultLights>

    <brakeLights>
      <brakeLight shaderNode="rearBreakVisRight"/>
      <brakeLight shaderNode="rearBreakVisLeft"/>
    </brakeLights>

    <turnLights>
      <turnLightLeft shaderNode="blinkerLeft" />
      <turnLightRight shaderNode="blinkerRight" />
    </turnLights>
  </lights>

  <sprayer fillUnitIndex="1" unloadInfoIndex="1" loadInfoIndex="1">
    <usageScales workingWidth="32" scale="1" />

    <effects>
      <!-- unloading spreader -->
      <effectNode effectNode="effectUnloadLeft" delay="0" materialType="unloading" fadeTime="1"/>
      <effectNode effectNode="effectUnloadRight" delay="0" materialType="unloading" fadeTime="1"/>
    </effects>

    <animationNodes>
      <animationNode node="spinnerLeft"  rotSpeed="-900"  turnOnFadeTime="1" turnOffFadeTime="2" />
      <animationNode node="spinnerRight" rotSpeed="900" turnOnFadeTime="1" turnOffFadeTime="2" />
    </animationNodes>


    <sprayTypes>
      <sprayType foldingConfigurationIndex="1" fillTypes="fertilizer">
        <usageScales workingWidth="32" />

        <effects>
          <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="effectNode">
            <motionPathEffect textureFilename="$data/vehicles/bredal/fertilizerEffectWideArray.dds" numRows="22" rowLength="32" minFade="0.03" isLeft="false"/>
          </effectNode>
        </effects>

        <ai>
          <areaMarkers leftNode="aiMarkerLeft" rightNode="aiMarkerRight" backNode="aiMarkerBack"/>
          <sizeMarkers leftNode="sizeLeft" rightNode="sizeRight" backNode="sizeBack"/>
        </ai>
      </sprayType>

      <sprayType foldingConfigurationIndex="1" fillTypes="lime">
        <usageScales workingWidth="24" />

        <effects>
          <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="effectNodeSmallLime">
            <motionPathEffect textureFilename="$data/vehicles/bredal/fertilizerEffectWideArray.dds" numRows="22" rowLength="32" minFade="0.03" isLeft="false"/>
          </effectNode>
          <effectNode effectNode="effectNodeSmallLimeParticleSmoke" delay="0.5" effectClass="ParticleEffect" particleType="smoke" materialType="smokeParticle" emitCountScale="5" worldSpace="true"/>
        </effects>

        <ai>
          <areaMarkers leftNode="aiMarkerLeft_Lime" rightNode="aiMarkerRight_Lime" backNode="aiMarkerBack_Lime"/>
          <sizeMarkers leftNode="sizeLeft" rightNode="sizeRight" backNode="sizeBack"/>
        </ai>
      </sprayType>
    </sprayTypes>
  </sprayer>

  <workAreas>
    <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" sprayType="1">
      <area startNode="areaStart" widthNode="areaWidth" heightNode="areaHeight"/>
    </workArea>
    <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" sprayType="2">
      <area startNode="areaStart_Lime" widthNode="areaWidth_Lime" heightNode="areaHeight_Lime"/>
    </workArea>
  </workAreas>

  <animations>
    <animation name="moveSupport">
      <part node="base_support" startTime="0.0" endTime="0.1" visibility="false"/>
      <part node="base_support" startTime="0.1" endTime="0.2" visibility="true"/>
      <part node="supportVis" startTime="0.5" endTime="1.0" startTrans="0 0.75 0" endTrans="0 0.1 0"/>



      <part node="leg_COL" startTime="0.5" endTime="1.0" startTrans="-0.248 1.3 0.502" endTrans="-3.248 0.388 0.502"/>
      <part node="leg_COL" startTime="0.0" endTime="0.5" startTrans="-0.248 1.3 0.502" endTrans="-3.248 0.388 0.502"/>
      <part node="leg_COL2" startTime="0.5" endTime="1.0" startTrans="0.248 1.3 0.502" endTrans="3.248 0.388 0.502"/>
      <part node="leg_COL2" startTime="0.0" endTime="0.5" startTrans="0.248 1.3 0.502" endTrans="3.248 0.388 0.502"/>
      <part node="leg_COL3" startTime="0.5" endTime="1.0" startTrans="0.248 1.3 -1.926" endTrans="3.248 0.388 -1.926"/>
      <part node="leg_COL3" startTime="0.0" endTime="0.5" startTrans="0.248 1.3 -1.926" endTrans="3.248 0.388 -1.926"/>
      <part node="leg_COL4" startTime="0.5" endTime="1.0" startTrans="-0.248 1.3 -1.926" endTrans="-3.248 0.388 -1.926"/>
      <part node="leg_COL4" startTime="0.0" endTime="0.5" startTrans="-0.248 1.3 -1.926" endTrans="-3.248 0.388 -1.926"/>
    </animation>
  </animations>

  <fillUnit>
    <fillUnitConfigurations>
      <fillUnitConfiguration>
        <fillUnits>
          <unloading width="32" offset="0 0 0"/>
          <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="fertilizer lime" capacity="19200">
            <exactFillRootNode node="exactFillRootNode"/>
            <fillRootNode node="0>"/>
            <fillMassNode node="0>"/>
            <autoAimTargetNode node="autoAimFillTargetNode"/>
          </fillUnit>
        </fillUnits>
      </fillUnitConfiguration>
    </fillUnitConfigurations>
  </fillUnit>
  <fillVolume>
    <fillVolumeConfigurations>
      <fillVolumeConfiguration>
        <volumes>
          <volume node="fillVolume" maxDelta="0.2" maxAllowedHeapAngle="18" fillUnitIndex="1" />
        </volumes>
      </fillVolumeConfiguration>
    </fillVolumeConfigurations>
  </fillVolume>

  <ai>
    <needsLowering value="false"/>
    <agentAttachment width="3" height="2.0" length="3" lengthOffset="0"/>
    <collisionTrigger node="aiCollisionNode" width="3.0" height="3.1"/>
  </ai> 

  <designColorConfigurations useDefaultColors="false" price="0">
    <material materialSlotName="base_mat"/>
    <designColorConfiguration materialTemplateName="silverScratched" price="0" isDefault="true" name="$l10n_ui_colorSilver"/>
    <designColorConfiguration materialTemplateName="JOHNDEERE_GREEN1" price="0" name="$l10n_ui_colorGreen"/>
  </designColorConfigurations>    

  <foliageBending>
    <bendingNode minX="-1" maxX="1" minZ="-3.3" maxZ="1" yOffset="1.75" />
  </foliageBending>

  <realDirtColor useSimpleMode="true" />

  <wearable wearDuration="480" workMultiplier="5" fieldMultiplier="2"/>
  <washable dirtDuration="90" washDuration="1" workMultiplier="4" fieldMultiplier="2"/>

