            </movingPart>
            <movingPart node="attacherWingPart" referencePoint="attacherWingRef" referenceFrame="attacherWingRef"/>
            <movingPart node="attacherHydraulic" referencePoint="attacherHydraulicRef" referenceFrame="attacherHydraulicRef" playSound="true">
                <translatingPart node="attacherHydraulicPunch" />
            </movingPart>

            <movingPart node="attacherRotationIndicator" referencePoint="attacherRotationIndicatorRef" referenceFrame="attacherRotationIndicatorRef" doInversedLineAlignment="true" doDirectionAlignment="false">
                <orientationLine>
                    <lineNode node="attacherRotationIndicatorLine01"/>
                    <lineNode node="attacherRotationIndicatorLine02"/>
                </orientationLine>
            </movingPart>

            <movingPart node="attacherLockSpring" referencePoint="attacherLockSpringRef" referenceFrame="attacherLockSpringRef" scaleZ="true"/>
        </movingParts>
        <sounds>
            <actionSound template="hydraulicOpen06"   pitchScale="0.4" volumeScale="0.4" linkNode="armPhys"      actionNames="TOOL_MOVE_POS"               nodes="armPhys"  fadeIn="0.1" fadeOut="0.05"/>
            <actionSound template="hydraulicOpen02"   pitchScale="1.4" volumeScale="2.2" linkNode="armPhys"      actionNames="TOOL_MOVE_POS"               nodes="armPhys"  fadeIn="0.1" fadeOut="0.05"/>
            <actionSound template="hydraulicClose03"  pitchScale="1.7" volumeScale="2.9" linkNode="armPhys"      actionNames="TOOL_MOVE_NEG"               nodes="armPhys"  fadeIn="0.1" fadeOut="0.05"/>
            <actionSound template="hydraulicOpen06"   pitchScale="1.0" volumeScale="0.5" linkNode="armPhys"      actionNames="TOOL_MOVE_NEG"               nodes="armPhys"  fadeIn="0.1" fadeOut="0.05"/>
            <actionSound template="hydraulicClose01"  pitchScale="1.7" volumeScale="1.6" linkNode="attacherPhys" actionNames="TOOL_MOVE_POS TOOL_MOVE_NEG" nodes="attacherPhys" fadeIn="0.1" fadeOut="0.05"/>
            <actionSound template="metalLowVar2"      pitchScale="1.0" volumeScale="3"   linkNode="armPhys"      actionNames="TOOL_MOVE_END_POS_LIMIT"     nodes="armPhys"/>
