            <actionSound template="metalClose"        pitchScale="2.5" volumeScale="0.2" linkNode="armPhys"      actionNames="TOOL_MOVE_END_POS_LIMIT"     nodes="armPhys"/>
            <actionSound template="clackVar6"         pitchScale="1.0" volumeScale="1.0" linkNode="armPhys"      actionNames="TOOL_MOVE_END_POS_LIMIT"     nodes="armPhys"/>
            <actionSound template="metalLowVar2"      pitchScale="1.3" volumeScale="2.0" linkNode="armPhys"      actionNames="TOOL_MOVE_END_NEG_LIMIT"     nodes="armPhys"/>
            <actionSound template="cylinderPunch"     pitchScale="1.8" volumeScale="0.2" linkNode="armPhys"      actionNames="TOOL_MOVE_END_NEG_LIMIT"     nodes="armPhys"/>
            <actionSound template="metalClose"        pitchScale="2.8" volumeScale="0.1" linkNode="armPhys"      actionNames="TOOL_MOVE_END_NEG_LIMIT"     nodes="armPhys"/>
            <actionSound template="clackVar6"         pitchScale="1.8" volumeScale="0.5" linkNode="armPhys"      actionNames="TOOL_MOVE_END_NEG_LIMIT"     nodes="armPhys"/>
        </sounds>
    </cylindered>

