    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" hardAttach="true" jointType="attachableFrontloader">
                <objectChange node="quickConnectorAttached" visibilityActive="true" visibilityInactive="false" translationActive="0.071 -0.008 -0.029" rotationActive="-7 0 0"/>
                <objectChange node="hosesAttached"          visibilityActive="true" visibilityInactive="false"/>
                <objectChange node="quickConnectorDetached" visibilityActive="false" visibilityInactive="true"/>
            </inputAttacherJoint>
        </inputAttacherJoints>
        <support animationName="moveSupport" />		
    </attachable>

    <attacherJoints>
        <attacherJoint jointType="frontloader" node="attacherJointTools" nodeVisual="attacherJointTools_visuals" allowsJointLimitMovement="false" allowsLowering="false" rotLimitSpring="7500 7500 7500" rotLimitDamping="1 1 1" transLimitSpring="7500 7500 7500" transLimitDamping="1 1 1">
            <schema position="1 0" rotation="0" invertX="false" />
            <objectChange node="attacherLock" translationActive="0 0 0" translationInactive="0.077 0 0" />
        </attacherJoint>
        <sounds>
            <attach template="attach01" linkNode="frontloaders683R_main_component1" />
        </sounds>
    </attacherJoints>

    <connectionHoses>
        <target attacherJointIndices="1" type="hydraulicIn" node="hydraulicIn01_socket" socket="hydraulic01" socketMaterialTemplateName="JOHNDEERE_YELLOW1"/>
        <target attacherJointIndices="1" type="hydraulicOut" node="hydraulicOut01_socket" socket="hydraulic01" socketMaterialTemplateName="JOHNDEERE_YELLOW1"/>
    </connectionHoses>

    <animations>
        <animation name="moveSupport">
            <part node="supportArm01" startTime="0.0" endTime="1.0" startRot="71.5 0 0" endRot="0 0 0" />
            <part node="supportArm02" startTime="0.0" endTime="1.0" startRot="140 0 0" endRot="0 0 0" />

            <sound template="hingeMedium02" startTime="0.1"  direction="1"  volumeScale="2" pitchScale="1.2" />
            <sound template="hingeMedium03" startTime="0.9"  direction="-1" volumeScale="2" pitchScale="1.2" />
            <sound template="hingeImp"      startTime="0.1"  direction="-1" volumeScale="0.5" pitchScale="1" />
            <sound template="vibratingImp"  startTime="0.01" direction="-1" volumeScale="0.6" pitchScale="1.7" />
            <sound template="clackVar6"     startTime="0.01" direction="-1" volumeScale="2" pitchScale="1" />
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="armPhys" delayedNode="arm">
                <controls axis="AXIS_FRONTLOADER_ARM" invertAxis="true" mouseSpeedFactor="0.3" iconName="FRONTLOADER_ARM_ROTATE" />
                <attacherJoint jointIndices="1" />
                <rotation rotSpeed="25" rotAcceleration="125" rotMin="-75" rotMax="3" startRot="0" detachingRotMinLimit="-10" />
                <dependentPart node="attacherRotPhys" />
                <dependentMovingTool node="attacherPhys" minRotLimits="-46 -74" maxRotLimits="139 63"/>
                <dependentAnimation name="armHosesAnimation" rotationAxis="1" minValue="-75" maxValue="3" invert="true" />
            </movingTool>
            <movingTool node="attacherPhys" delayedNode="attacher">
                <controls axis="AXIS_FRONTLOADER_TOOL" invertAxis="true" mouseSpeedFactor="0.3" iconName="FRONTLOADER_ARM_ROTATE_TOOL" />
                <attacherJoint jointIndices="1" />
                <rotation rotSpeed="55" rotAcceleration="300" rotMin="-60" rotMax="100" startRot="0" syncMaxRotLimits="true" />
                <dependentAnimation name="attacherHosesAnimation" rotationAxis="1" minValue="-60" maxValue="100" invert="false" />
            </movingTool>
            <movingTool node="arm">
                <dependentPart node="armHydraulic" />
                <dependentPart node="attacherRot" />
                <dependentPart node="armAlign" />
                <dependentPart node="attacherRotationIndicator" />
            </movingTool>
            <movingTool node="attacher">
                <dependentPart node="attacherWing" />
                <dependentPart node="attacherHydraulic" />
                <dependentPart node="attacherRotationIndicator" />
            </movingTool>

            <movingTool node="attacherLock">
                <dependentPart node="attacherLockSpring" />
            </movingTool>
        </movingTools>

        <movingParts>
            <movingPart node="attacherRotPhys" referenceFrame="frontloaders683R_main_component1">
                <attacherJoint jointIndices="1" />
            </movingPart>
            <movingPart node="attacherRot" referenceFrame="frontloaders683R_main_component1">
                <attacherJoint jointIndices="1" />
                <dependentPart node="attacherWing" />
            </movingPart>

            <movingPart node="armHydraulic" referencePoint="armHydraulicRef" referenceFrame="armHydraulicRef">
                <translatingPart node="armHydraulicPunch" />
            </movingPart>
            <movingPart node="armAlign" localReferencePoint="armAlignPart" referencePoint="armAlignPartRef" referenceFrame="armAlignRefFrame">
                <dependentPart node="armAlignPart" />
                <dependentPart node="attacherHydraulic" />
            </movingPart>
            <movingPart node="armAlignPart" referencePoint="armAlignPartRef" referenceFrame="armAlignPartRef"/>
            <movingPart node="attacherWing" localReferencePoint="attacherWingPart" referencePoint="attacherWingRef" referenceFrame="attacherWingRefFrame">
