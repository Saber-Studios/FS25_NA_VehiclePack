<?xml version="1.0" encoding="utf-8" standalone="no"?>
<vehicle type="spreader">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>
    <storeData>
        <name>10 CU FT GATOR FERTI</name>		
        <functions>
            <function>$l10n_function_fertilizer</function>
        </functions>
        <specs>
            <combination xmlFilename="Gator.xml" />	
            <combination xmlFilename="Gator2x4.xml" />	
            <combination xmlFilename="BladeLeveler.xml" />	
            <combination xmlFilename="BladeSnow.xml" />	
            <workingWidth>10.0</workingWidth>			
        </specs>		
        <image>store_Ferti.dds</image>
        <price>2500</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>JOHNDEERE</brand>
        <category>fertilizerSpreaders</category>
        <vertexBufferMemoryUsage>770304</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>96256</indexBufferMemoryUsage>
        <textureMemoryUsage>2162688</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>	
	
	
    <base>
        <typeDesc>$l10n_typeDesc_fertilizerSpreader</typeDesc>
    <filename>Salt.i3d</filename>
        <size width="3.5" length="8" height="1.6" lengthOffset="0"/>
       <speedLimit value="20"/>		
        <components>
            <component centerOfMass="0 0.2 0.35" solverIterationCount="10" mass="500"/>		
        </components>
        <schemaOverlay attacherJointPosition="0 0" name="IMPLEMENT" />
    </base>	
    <powerConsumer ptoRpm="450" neededMaxPtoPower="10"/>	
    <wheels>
    </wheels>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="GatorLift" upperRotationOffset="0"/>
            <inputAttacherJoint node="attacherJoint" jointType="GatorLift4x2" upperRotationOffset="0">		
            <objectChange node="Added2x4" visibilityActive="true" visibilityInactive="false" />				
        </inputAttacherJoint>				
        </inputAttacherJoints>
        <support animationName="moveSupport" delayedOnAttach="false"/>	
    </attachable>

    <workAreas>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false">
            <area startNode="workAreaStart" widthNode="workAreaWidth" heightNode="workAreaHeight" />
            <onlyActiveWhenLowered value="false"/>
        </workArea>
    </workAreas>	

    <sprayer fillUnitIndex="1" usage="0.5" usageWorkArea="1">
        <usageScales workAreaIndex="1" scale="0.5" />
        <animationNodes>
            <animationNode node="spreaderAnimationNode" rotSpeed="1250" turnOnFadeTime="0.5" turnOffFadeTime="2" rotationAxis="2"/>			
        </animationNodes>

        <effects>
            <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="fertilizerEffectLeft">
                <motionPathEffect textureFilename="$data/vehicles/amazone/zats3200/fertilizerEffectLeftArray.dds" numRows="22" rowLength="32" minFade="0.03" isLeft="true"/>
            </effectNode>
            <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="fertilizerEffectRight">
                <motionPathEffect textureFilename="$data/vehicles/amazone/zats3200/fertilizerEffectRightArray.dds" numRows="22" rowLength="32" minFade="0.03" isLeft="false"/>
            </effectNode>
            <effectNode effectNode="unloadMesh" delay="0" materialType="unloading" fadeTime="1"/>			
        </effects>
    </sprayer>	

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration name="$l10n_configuration_valueDefault" price="0" >
                <fillUnits>
                    <unloading width="11" offset="0 0 0" />
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="fertilizer lime" capacity="1000" >
                        <exactFillRootNode node="exactFillRootNode" />
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>		
	
    <animations>						
        <!-- move support wheel down/up -->
        <animation name="moveSupport">
            <part node="supportCol1" startTime="0.0" endTime="0.001" startTrans="0.194 0.7 0.329" endTrans="0 0.7 0"/>
            <part node="supportCol1" startTime="0.001" endTime="0.01" startTrans="0 0.7 0" endTrans="0 0.0 0"/>			
            <part node="supportCol" startTime="0.0" endTime="0.001" startTrans="-0.194 0.7 0.329" endTrans="0 0.7 0"/>
            <part node="supportCol" startTime="0.001" endTime="0.01" startTrans="0 0.7 0" endTrans="0 0.0 0"/>							
            <part node="support" startTime="0" endTime="0.05" visibility="false"/>
            <part node="support" startTime="0.05" endTime="0.1" visibility="true"/>	

            <sound template="clackVar5" startTime="0.09" direction="-1" volumeScale="0.9" pitchScale="1.6" linkNode="attacherJoint"/>
            <sound template="latchSmall" startTime="0.19" direction="-1" volumeScale="0.2" pitchScale="1.2" linkNode="attacherJoint"/>
            <sound template="clackVar1" startTime="0.01" direction="1" volumeScale="1.5" pitchScale="1.6" linkNode="attacherJoint"/>			
        </animation>
    </animations>		
    <cylindered>
    <movingTools>			
        <movingTool node="attacherJoint">
            <inputAttacherJoint value="true" />		
        </movingTool>		
    </movingTools>
    </cylindered>

    <fillVolume>
        <fillVolumeConfigurations>
            <fillVolumeConfiguration>
                <volumes>
                    <volume node="fillVolume" maxDelta="0.34" fillUnitIndex="1" maxAllowedHeapAngle="16" />
                </volumes>
            </fillVolumeConfiguration>
        </fillVolumeConfigurations>
    </fillVolume>	
    <variableWorkWidth>
        <sections>
            <section isLeft="true" maxWidthNode="section01"/>
            <section isLeft="true" maxWidthNode="section02"/>
            <section isLeft="true" maxWidthNode="section03"/>

            <section isLeft="true" maxWidthNode="section04"/>
            <section isLeft="true" maxWidthNode="section05"/>
            <section isLeft="false" maxWidthNode="section06"/>

            <section isLeft="false" maxWidthNode="section08"/>
            <section isLeft="false" maxWidthNode="section09"/>
            <section isLeft="false" maxWidthNode="section10"/>

            <section isLeft="false" maxWidthNode="section11"/>
        </sections>

        <sectionNodes>
            <sectionNode node="workAreaHeight" isLeft="true"  minTrans="1 0 -4"  maxTrans="5 0 -2.313"/>
            <sectionNode node="workAreaWidth"  isLeft="false" minTrans="-1 0 -4" maxTrans="-5 0 -2.313"/>

            <sectionNode node="aiMarkerLeft"  isLeft="true"  minTrans="1 0 -4"  maxTrans="5 0 -2.313"/>
            <sectionNode node="aiMarkerRight" isLeft="false" minTrans="-1 0 -4" maxTrans="-5 0 -2.313"/>
            <sectionNode node="aiMarkerBack"  isLeft="false" minTrans="0 0 -3"    maxTrans="0 0 -1"/>
        </sectionNodes>
    </variableWorkWidth>

    <turnOnVehicle>
        <sounds>
            <work template="defaultSpreader" linkNode="3p_main_component1" />
        </sounds>
    </turnOnVehicle>	
    <ai>
        <needsLowering value="false" />
        <collisionTrigger node="aiCollisionNode" width="2.1" height="1.6"/>
        <areaMarkers leftNode="aiMarkerLeft" rightNode="aiMarkerRight" backNode="aiMarkerBack" />
        <sizeMarkers leftNode="sizeMarkerLeft" rightNode="sizeMarkerRight" backNode="sizeMarkerBack" />
        <agentAttachment width="2.0" height="1.6" length="2.2" lengthOffset="0"/>
    </ai>	

    <foliageBending>
        <bendingNode minX="-0.85" maxX="0.85" minZ="-0.5" maxZ="0.8" yOffset="0.22" />
    </foliageBending>		
    <baseColorConfigurations useDefaultColors="true" defaultColorIndex="26">
        <material materialSlotName="base_color_01" />
    </baseColorConfigurations>

	<designColorConfigurations useDefaultColors="true" defaultColorIndex="8">
		<material materialSlotName="base_color_02" />
	</designColorConfigurations>

    <wearable wearDuration="480" workMultiplier="5" fieldMultiplier="2"/>
    <washable dirtDuration="90" washDuration="1" workMultiplier="3" fieldMultiplier="2"/>	
    <i3dMappings>
        <i3dMapping id="3p_main_component1" node="0>" />		
        <i3dMapping id="moveSupport" node="0>1"/>
        <i3dMapping id="moveSupport1" node="0>1|0"/>
