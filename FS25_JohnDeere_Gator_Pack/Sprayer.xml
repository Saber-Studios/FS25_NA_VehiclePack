<?xml version="1.0" encoding="utf-8" standalone="no"?>
<vehicle type="sprayer" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>32A</name>
        <specs>
            <workingWidth>5.0</workingWidth>
            <combination xmlFilename="Gator.xml" />	
            <combination xmlFilename="Gator2x4.xml" />	
            <combination xmlFilename="BladeLeveler.xml" />	
            <combination xmlFilename="BladeSnow.xml" />				
        </specs>
        <functions>
            <function>$l10n_function_sprayer</function>
        </functions>		
        <image>store_Sprayer.dds</image>
        <price>2000</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>JOHNDEERE</brand>
        <category>sprayers</category>
        <vertexBufferMemoryUsage>1048832</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>122368</indexBufferMemoryUsage>
        <textureMemoryUsage>1835008</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>	
	
	
    <base>
        <typeDesc>$l10n_typeDesc_sprayer</typeDesc>
    <filename>Sprayer.i3d</filename>
        <size width="3.5" length="8" height="1.7" lengthOffset="0"/>
        <speedLimit value="12" />		
        <components>
            <component centerOfMass="0 0.2 0.35" solverIterationCount="10" mass="500"/>		
        </components>
        <schemaOverlay attacherJointPosition="0 0" name="IMPLEMENT" />
        <sounds volumeFactor="1.3" />
    </base>	
	
    <wheels>
    </wheels>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="GatorLift" upperRotationOffset="0"/>
            <inputAttacherJoint node="attacherJoint" jointType="GatorLift4x2" upperRotationOffset="0">		
            <objectChange node="Added2x4" visibilityActive="true" visibilityInactive="false" />				
        </inputAttacherJoint>		
        </inputAttacherJoints>
        <support animationName="moveSupport" delayedOnAttach="false"/>	
        <brakeForce force="0.5"/>
    </attachable>
		
	<workAreas>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" >
            <area startNode="workAreaStart" widthNode="workAreaWidth" heightNode="workAreaHeight" />
        </workArea>
    </workAreas>

    <foldable>
        <foldingConfigurations>
            <foldingConfiguration>
                <foldingParts startMoveDirection="1" turnOnFoldMaxLimit="0.1" turnOnFoldMinLimit="0">
                    <foldingPart animationName="folding" speedScale="0.7" />
                </foldingParts>
            </foldingConfiguration>
        </foldingConfigurations>
    </foldable>
		
    <turnOnVehicle turnOffIfNotAllowed="true" >
        <sounds>
            <work template="defaultSprayer" linkNode="Sprayer_main_component1" />
        </sounds>
    </turnOnVehicle>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits>
                    <unloading width="12" offset="0 0 0" />
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypeCategories="sprayer" capacity="400">
                        <exactFillRootNode node="exactFillRootNode"/>
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>	
	
    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeTrigger" emptySpeed="200" fillUnitIndex="1" allowAIDischarge="true">
            <info width="1.0" length="0.5" zOffset="0.4" />
            <trigger node="dischargeTrigger" />
        </dischargeNode>
    </dischargeable>
	
	<sprayer>
        <usageScales workingWidth="5.0" scale="1.5" />
        <effects>
            <effectNode effectNode="effects01"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects02"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects03"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects04"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects05"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects06"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects07"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects08"  delay="0.2" materialType="sprayer" />
            <effectNode effectNode="effects09"  delay="0.2" materialType="sprayer" />
        </effects>
    </sprayer>
    <variableWorkWidth>
        <sections>
        </sections>

        <sectionNodes>
            <sectionNode node="workAreaStart"  isLeft="true"  minTransX="2.5"  maxTransX="2.5" />
            <sectionNode node="workAreaWidth"  isLeft="false" minTransX="-2.5" maxTransX="-2.5" />
            <sectionNode node="workAreaHeight" isLeft="true"  minTransX="2.5"  maxTransX="2.5" />
            <sectionNode node="aiMarkerLeft"   isLeft="true"  minTransX="2.5"  maxTransX="2.5" />
            <sectionNode node="aiMarkerRight"  isLeft="false" minTransX="-2.5" maxTransX="-2.5" />
        </sectionNodes>
    </variableWorkWidth>	
    <animations>						
        <!-- move support wheel down/up -->
        <animation name="moveSupport">
            <part node="supportCol1" startTime="0.0" endTime="0.001" startTrans="0.194 0.7 0.329" endTrans="0 0.7 0"/>
            <part node="supportCol1" startTime="0.001" endTime="0.01" startTrans="0 0.7 0" endTrans="0 0.0 0"/>			
            <part node="supportCol" startTime="0.0" endTime="0.001" startTrans="-0.194 0.7 0.329" endTrans="0 0.7 0"/>
            <part node="supportCol" startTime="0.001" endTime="0.01" startTrans="0 0.7 0" endTrans="0 0.0 0"/>							
            <part node="support" startTime="0" endTime="0.05" visibility="false"/>
            <part node="support" startTime="0.05" endTime="0.1" visibility="true"/>			
        </animation>
        <animation name="folding">
			<part node="LefttArm"           	startTime="0" endTime="1" startRot="0 0 0" endRot="0 0 90" />
			<part node="RightArm"           	startTime="0" endTime="1" startRot="0 0 0" endRot="0 0 -90" />
        <!-- sounds -->
            <!-- <sound template="HydraulicOpen02" startTime="0"   endTime="1" volumeScale="1.7" pitchScale="0.6" fadeIn="0.1" fadeOut="0.1" /> -->
            <sound template="hydraulicClose02" startTime="0"   endTime="1" volumeScale="1.7" pitchScale="0.6" fadeIn="0.1" fadeOut="0.1" />
        </animation> 		
    </animations>		
    <cylindered>
    <movingTools>			
        <movingTool node="attacherJoint">
            <inputAttacherJoint value="true" />		
        </movingTool>	
        <movingTool node="LefttArm" >
            <dependentPart node="Moving01" />	
            <dependentPart node="Moving03" />			
        </movingTool>		
    </movingTools>
    <movingParts>
			<movingPart node="Moving01" referencePoint="Moving02" referenceFrame="Moving02" scaleZ="true" />
			<movingPart node="Moving03" referencePoint="Moving04" referenceFrame="Moving04" scaleZ="true" />			
    </movingParts> 	
		</cylindered>
	
    <ai>
        <areaMarkers leftNode="aiMarkerLeft" rightNode="aiMarkerRight" backNode="aiMarkerBack" />
        <needsLowering value="false"/>
        <collisionTrigger node="aiCollisionTrigger" width="5.0" height="3.5"/>
        <allowTurnBackward value="false" />
        <agentAttachment width="2.5" height="3.5" length="2.2" lengthOffset="-0.2"/>
    </ai>	

    <foliageBending>
        <bendingNode minX="-0.85" maxX="0.85" minZ="-1.45" maxZ="1.5" yOffset="0.22" />
    </foliageBending>		
    <baseColorConfigurations useDefaultColors="true" defaultColorIndex="26">
        <material materialSlotName="base_color_01" />
    </baseColorConfigurations>	

	<designColorConfigurations useDefaultColors="true" defaultColorIndex="8">
		<material materialSlotName="base_color_02" />
	</designColorConfigurations>

    <wearable wearDuration="480" workMultiplier="5" fieldMultiplier="2"/>
    <washable dirtDuration="90" washDuration="1" workMultiplier="3" fieldMultiplier="2"/>	
    <i3dMappings>
        <i3dMapping id="Sprayer_main_component1" node="0>" />	
        <i3dMapping id="attacherMove" node="0>0|1" />		
        <i3dMapping id="moveSupport" node="0>1"/>
        <i3dMapping id="moveSupport1" node="0>1|0"/>
        <i3dMapping id="supportCol" node="0>1|0|0"/>
        <i3dMapping id="moveSupport2" node="0>1|1"/>
        <i3dMapping id="supportCol1" node="0>1|1|0"/>
        <i3dMapping id="support" node="0>1|2"/>		
        <i3dMapping id="attacherJoint" node="0>0|1|0" />	
        <i3dMapping id="mainTool" node="0>0|0"/>	
        <i3dMapping id="LefttArm" node="0>0|2|0"/>	
        <i3dMapping id="RightArm" node="0>0|2|1"/>	
        <i3dMapping id="workAreaStart" node="0>2|0" />
        <i3dMapping id="workAreaWidth" node="0>2|1" />
        <i3dMapping id="workAreaHeight" node="0>2|2" />
        <i3dMapping id="aiMarkers" node="0>3|0" />
        <i3dMapping id="aiMarkerLeft" node="0>3|0|0" />
        <i3dMapping id="aiMarkerRight" node="0>3|0|1" />
        <i3dMapping id="aiMarkerBack" node="0>3|0|2" />
        <i3dMapping id="sizeMarkerLeft" node="0>3|1|0" />
        <i3dMapping id="sizeMarkerRight" node="0>3|1|1" />
        <i3dMapping id="sizeMarkerBack" node="0>3|1|2" />
        <i3dMapping id="aiCollisionTrigger" node="0>3|2" />
        <i3dMapping id="exactFillRootNode" node="0>0|4|0" />
        <i3dMapping id="dischargeTrigger" node="0>0|4|1" />	
        <i3dMapping id="effects01" node="0>0|2|2" />
        <i3dMapping id="effects02" node="0>0|2|3" />
        <i3dMapping id="effects03" node="0>0|2|4" />
        <i3dMapping id="effects04" node="0>0|2|0|0" />
        <i3dMapping id="effects05" node="0>0|2|0|1" />
        <i3dMapping id="effects06" node="0>0|2|0|2" />	
        <i3dMapping id="effects07" node="0>0|2|1|0" />
        <i3dMapping id="effects08" node="0>0|2|1|1" />
        <i3dMapping id="effects09" node="0>0|2|1|2" />	
        <i3dMapping id="Spayer" node="0>0|2" />		
        <i3dMapping id="Moving01" node="0>0|2|5" />	
        <i3dMapping id="Moving02" node="0>0|2|0|3" />
        <i3dMapping id="Moving03" node="0>0|2|7" />
        <i3dMapping id="Moving04" node="0>0|2|1|3" />	
        <i3dMapping id="Added2x4" node="0>0|3" />			
    </i3dMappings>
</vehicle>	
