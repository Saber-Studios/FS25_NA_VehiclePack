<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="fuelTrailer" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../bin/shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>
    <storeData>
        <name>
			<en>Fuel Transfer Tank</en>
			<de>Kraftstoff-Transfertank</de>
			<fr>Réservoir de transfert de carburant</fr>
		</name>
        <specs>
            <combination xmlFilename="Gator.xml" />	
            <combination xmlFilename="Gator2x4.xml" />	
        </specs>
        <functions>
            <function>$l10n_function_tank</function>
        </functions>
        <image>store_MobileTank.png</image>
        <price>1000</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>JOHNDEERE</brand>
        <category>misc</category>
        <vertexBufferMemoryUsage>1259008</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>144384</indexBufferMemoryUsage>
        <textureMemoryUsage>2424832</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_tipper</typeDesc>
        <filename>MobileTank.i3d</filename>
        <size width="3.5" length="8" height="1.4" lengthOffset="0"/>
        <components maxMass="500">
            <component centerOfMass="0 0.2 0.45" solverIterationCount="10" mass="500"/>	
        </components>
        <schemaOverlay attacherJointPosition="0 0" name="IMPLEMENT" />
        <mapHotspot type="TRAILER" />
    </base>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="GatorLift" upperRotationOffset="0">
            <objectChange node="Tipper6x4" visibilityActive="true" visibilityInactive="true" />
            <objectChange node="Tipper4x2" visibilityActive="false" visibilityInactive="false" />		
            <objectChange node="Decals6x4" visibilityActive="true" visibilityInactive="true" />	
            <objectChange node="Decals4x2" visibilityActive="false" visibilityInactive="false" />				
        </inputAttacherJoint>	
            <inputAttacherJoint node="attacherJoint" jointType="GatorLift4x2" upperRotationOffset="0">
            <objectChange node="Tipper6x4" visibilityActive="false" visibilityInactive="true" />		
            <objectChange node="Tipper4x2" visibilityActive="true" visibilityInactive="false" />			
            <objectChange node="Decals6x4" visibilityActive="false" visibilityInactive="true" />		
            <objectChange node="Decals4x2" visibilityActive="true" visibilityInactive="false" />					
        </inputAttacherJoint>		
        </inputAttacherJoints>
        <support animationName="moveSupport" delayedOnAttach="false"/>	
        <brakeForce force="0.5"/>
    </attachable>
	

	
    <animations>
		<!-- backtipping -->
        <animation name="movingTool2">
            <part node="Door"       startTime="0.03" endTime="1.00" startRot="0 0 0" endRot="-95 0 0"/>

            <sound template="metalClose"    startTime="0.15"  direction="-1" volumeScale="1.4" pitchScale="1.4"  linkNode="Door"/>
            <sound template="metalOpen"     startTime="0.15"  direction="1"  volumeScale="1.6" pitchScale="0.8"  linkNode="Door"/>
            <sound template="hingeMedium"   startTime="0.90"  direction="-1"  volumeScale="0.8" pitchScale="1.1" linkNode="Door"/>			
        </animation>						
        <!-- move support wheel down/up -->
        <animation name="moveSupport">
            <part node="supportCol1" startTime="0.0" endTime="0.001" startTrans="0.194 0.7 0.329" endTrans="0 0.7 0"/>
            <part node="supportCol1" startTime="0.001" endTime="0.01" startTrans="0 0.7 0" endTrans="0 0.0 0"/>			
            <part node="supportCol" startTime="0.0" endTime="0.001" startTrans="-0.194 0.7 0.329" endTrans="0 0.7 0"/>
            <part node="supportCol" startTime="0.001" endTime="0.01" startTrans="0 0.7 0" endTrans="0 0.0 0"/>							
            <part node="support" startTime="0" endTime="0.05" visibility="false"/>
            <part node="support" startTime="0.05" endTime="0.1" visibility="true"/>	
            <sound template="clackVar1"   startTime="0.4"  direction="1"  volumeScale="1.5" pitchScale="1.5" linkNode="attacherJoint"/>
            <sound template="metalClose"  startTime="0.1"  direction="-1" volumeScale="1.2" pitchScale="2"   linkNode="attacherJoint"/>
            <sound template="latchSmall"  startTime="0.49"                volumeScale="0.2" pitchScale="1.4" linkNode="attacherJoint"/>				
        </animation>
        <animation name="fillLevel">
            <part node="eguille" startTime="0" endTime="1" startRot="0 0 0" endRot="0 -92 0" />
        </animation>		
    </animations>

	
    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits>
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="diesel" capacity="500" >		
                        <dashboard displayType="ANIMATION" valueType="fillLevel" animName="fillLevel" />
                        <exactFillRootNode node="exactFillRootNode"/>
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>
	
    <cylindered>
    <movingTools>		
		<!-- backTipping -->	
        <movingTool node="attacherJoint">
            <inputAttacherJoint value="true" />			
        </movingTool>	
        <movingTool node="attacherMove">
            <inputAttacherJoint value="true" />
        </movingTool>		
    </movingTools>
    <movingParts>  		
        </movingParts>
    </cylindered>
    <foldable>
        <foldingConfigurations>
            <foldingConfiguration price="0" >
                <foldingParts requiresPower="false">
                    <foldingPart animationName="movingTool2" speedScale="1.3" />
                </foldingParts>
            </foldingConfiguration>
        </foldingConfigurations>
    </foldable>				


    <fillTriggerVehicle triggerNode="fuelTrigger" fillUnitIndex="1" litersPerSecond="30"/>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeTrigger" emptySpeed="20" fillUnitIndex="1" canDischargeToGround="false" allowAIDischarge="true">
            <info width="1.0" length="0.5" zOffset="0.4" />
            <trigger node="dischargeTrigger" />
        </dischargeNode>
    </dischargeable>

    <tipOccluder>
        <occlusionArea start="tipOcclusionArea1Start" width="tipOcclusionArea1Width" height="tipOcclusionArea1Height" />
    </tipOccluder>
    <supportsFillTriggers value="true" />

    <ai>
        <allowTurnBackward value="false"/>
        <turningRadiusLimitation radius="9"/>
        <agentAttachment width="1" height="1.0" length="0.6" lengthOffset="-0.15"/>
    </ai>

    <foliageBending>
        <bendingNode minX="-0.85" maxX="0.85" minZ="-0.5" maxZ="0.8" yOffset="0.22" />
    </foliageBending>

    <baseColorConfigurations useDefaultColors="true" defaultColorIndex="26">
        <material materialSlotName="base_color_01" />
    </baseColorConfigurations>
    <wearable wearDuration="480" workMultiplier="5" fieldMultiplier="2"/>
    <washable dirtDuration="90" washDuration="1" workMultiplier="3" fieldMultiplier="2"/>

    <i3dMappings>	
        <i3dMapping id="Bale_main_component1" node="0>" />	
        <i3dMapping id="attacherMove" node="0>0|1" />
		<i3dMapping id="attacherJoint" node="0>0|1|0" />	
        <i3dMapping id="moveSupport" node="0>1"/>
        <i3dMapping id="moveSupport1" node="0>1|0"/>
        <i3dMapping id="supportCol" node="0>1|0|0"/>
        <i3dMapping id="moveSupport2" node="0>1|1"/>
        <i3dMapping id="supportCol1" node="0>1|1|0"/>
        <i3dMapping id="support" node="0>1|2"/>		
        <i3dMapping id="movingTool1" node="0>1|2|0"/>
        <i3dMapping id="movingTool2" node="0>0|0"/>
        <i3dMapping id="movingTool3" node="0>0|0|1"/>		
        <i3dMapping id="Tipper6x4" node="0>0|0|0" />	
        <i3dMapping id="Tipper4x2" node="0>0|0|1" />	
        <i3dMapping id="Decals6x4" node="0>0|0|4|2" />	
        <i3dMapping id="Decals4x2" node="0>0|0|4|3" />
        <i3dMapping id="Door" node="0>0|0|4" />
		<i3dMapping id="fuelTrigger" node="0>3|0"/>
		<i3dMapping id="dischargeTrigger" node="0>3|1"/>
        <i3dMapping id="exactFillRootNode" node="0>3|2" />
		<i3dMapping id="tipOcclusionArea1Start" node="0>3|3|0|0"/>
		<i3dMapping id="tipOcclusionArea1Width" node="0>3|3|0|1"/>
		<i3dMapping id="tipOcclusionArea1Height" node="0>3|3|0|2"/>
        <i3dMapping id="MobileTank_vis" node="0>0|2"/>	

		<i3dMapping id="eguille" node="0>0|2|0|0|0"/>

    </i3dMappings>
</vehicle>
